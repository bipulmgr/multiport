{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "dev:multi": "bun src/multi-port.ts",
    "dev:ports": "bun src/multi-port.ts --default",
    "dev:config": "bun src/multi-port-config.ts",
    "dev:testing": "bun src/multi-port-config.ts --profile testing",
    "dev:staging": "bun src/multi-port-config.ts --profile staging",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts"
  },
  "dependencies": {
    "bun-plugin-tailwind": "^0.0.14",
    "react": "^19",
    "react-dom": "^19",
    "tailwindcss": "^4.0.6"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
